    		<SampleData4:SampleNotificationSettingsViewModel x:Key="SampleNotificationSettingsViewModel" d:IsDataSource="True"/>
			
    		
         
         
         
    		<SampleData2:SampleRoomViewModel x:Key="SampleRoomViewModel" d:IsDataSource="True"/>
         
    		<SampleData:SampleMainViewModel x:Key="SampleMainViewModel" d:IsDataSource="True"/>
         
