    			<Setter Property="Foreground" Value="#FF0297C4"/>
    			<Setter Property="FontWeight" Value="Normal"/>
    		</Style>
    		<Color x:Key="FadedOutTextBoxTextColor">#FF8C8C8C</Color>
		
